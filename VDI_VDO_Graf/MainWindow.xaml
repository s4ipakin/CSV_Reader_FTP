<Window x:Class="VDI_VDO_Graf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VDI_VDO_Graf"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1600">
    <Grid>
        <StackPanel Height="48" VerticalAlignment="Bottom" RenderTransformOrigin="0.499,0.542" Orientation="Horizontal">
            <Button Content="Температура" MinWidth="200" Click="MainBtcClck"/>
            <Button Content="Давление" MinWidth="200" Margin="10,0,0,0" Click="PressureBtnClk"/>
            <Button Content="Электропроводность" MinWidth="200" Margin="10,0,0,0" Click="btnCondClck" />
            <Button Content="Скорость протока" MinWidth="200" Margin="10,0,0,0" Click="btnFlawClck" />
            <Button Content="Уровень" MinWidth="200" Margin="10,0,0,0" Click="btnLewelClck" />
            <Button Content="Содержание углерода" MinWidth="200" Margin="10,0,0,0" Click="Button_Click" />
            <Button Content="Указать лог-файл" MinWidth="100" Margin="10,0,0,0" Click="btnChooseFile_clk" />
        </StackPanel>
        <Frame Name="Main" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
